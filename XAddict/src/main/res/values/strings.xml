    <string name="coke_short2"> - Dilated pupils </string>
    <string name="coke_short3"> - Increase in body temperature</string>
    <string name="coke_long1"> - Increased risk for stroke and seizures </string>
    <string name="coke_long2"> - Movement disorders; Parkinson\'s disease</string>
    <string name="coke_long3"> - Intracerebral hemorrhage</string>

    <string name="synth_desc">Is a synthetic drug that alters mood and perception. It is most commonly snorted like powder, taken as a capsule or tablet, or swallowed in liquid form.  </string>
